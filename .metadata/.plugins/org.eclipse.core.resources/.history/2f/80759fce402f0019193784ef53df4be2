import java.util.Scanner;

public class FibonnaciSeries {

	public static void main(String[] args) {
		
		Scanner in = new Scanner(System.in);
		int num = in.nextInt();
		
		int i = 0, j = 1, s = 0;
		
		for(int x = 1; x<= num; i++) {
			 s = i + j;
			
			i++;
			j++;
		}
		
		System.out.println(s);
	}

}
